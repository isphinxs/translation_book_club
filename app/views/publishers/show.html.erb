<h1><%= @publisher.name %></h1>
<% if @publisher.books.empty? %>
    <p>This publisher has no books.</p>
<% else %>
    <% @publisher.books.each do |book| %>
        <p><%= link_to book.name, book_path(book.id) %></p>
    <% end %>
<% end %>

<%= link_to "Edit", edit_publisher_path(@publisher) %>
